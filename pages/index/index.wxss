/**index.wxss**/
page {
  background: #f2f2f2;
  width: 100%;
  height: 100%;
}
.active2 {
  background: #333;
  color: white;
}
.padding {
  padding-bottom: 120rpx;
}
.selected {
  background: white;
  border-left: 8rpx solid #FFCC33;
}
.P_active {
  border-bottom: 4rpx solid #FFCC33;
}
.bottomfixed {
  background: white
}
.page1 {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.page1 .head {
  background: #FFCC33;
  padding: 20rpx 40rpx 20rpx;
  height: 240rpx;
}

.page1 .head .profile-info {
  background-color: #FFCC33;
  color: #000;
  display: flex;
  align-items: center;
  font-size: 28rpx;
}

.page1 .head .profile-info .avatar {
  height: 148rpx;
  width: 148rpx;
  border-radius: 50%;
  border: solid 12rpx white;
}

.page1 .head .profile-info .info {
  flex: 1;
  height: 85rpx;
  padding-left: 31.25rpx;
}

.page1 .head .profile-info .name {
  display: block;
  height: 45rpx;
  line-height: 45rpx;
  color: #000;
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
  overflow: hidden;
}

.page1 .head .profile-info .desc {
  display: block;
  height: 30rpx;
  line-height: 30rpx;
  margin-bottom: 10rpx;
  color: #333300;
  font-size: 28rpx;
}
.page1 .one,.two {
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
}
.page1 .saoma {
  font-size: 36rpx;
  font-weight: bold;
}
.page1 .address {
  font-size: 32rpx;
  line-height: 40rpx;
  overflow: hidden;
  text-align: right;
}
.P_input {
  position: relative;
}
.P_input input {
  background: white;
  border-radius: 40rpx;  
  width: 90%;
  padding: 10rpx 0;
  padding-left: 40rpx;
  margin: 40rpx 0 5% 20rpx;
}
.P_input image {
  position: absolute;
  z-index: 999999;
  width: 36rpx;
  height: 36rpx;
  padding: 6rpx 20rpx;
  right: 20rpx;
  top: 12rpx;
}
.anouncement{
  padding: 10rpx 0rpx;
  height: 50rpx;
}
.swiper-item,.swiperList,swiper  {
  height: 50rpx;
}
.icon-news {
  color: #ff5400;
}
.P_title {
  display: flex; 
  padding: 0 10%;
  height: 100rpx;
  justify-content: space-between;
  background: white;
  font-size: 36rpx;
}
.P_order,.P_business {
  width: 30%;
  padding: 30rpx;  
  text-align: center;
}
.orderPage-sideBar {
  width: 170rpx;
  overflow: hidden;
  font-size: 32rpx;
}
.P_order_P {
  margin-top: 4rpx;
}
.orderPage-sideBar,orderPage-sideBar2 {  
  float: left;
  height: 65%;
  overflow: scroll;
}
.orderPage-sideBar2 {
  width: 580rpx;
}
.menuList>view {
  padding: 20rpx 0;
  text-align: center;
  overflow: hidden;
}
.menuList2 {
  background: white;
  display: flex;
  align-items: center;
}
.menuList2 .P_message {
  text-align: left;
  margin-left: 20rpx;
  padding: 0;
  border: 0;
}
.menuList2>view {
  line-height: 35rpx;
}
.P_small {
  color: #999;
  font-size: 25rpx;
}
.line_t {
  text-decoration: line-through;
  font-size: 28rpx;
  color: #999;
  margin-right: 10rpx;
}
.P_bottom .iconfont {
  font-size: 35rpx;
  color: #FFCC33;
  padding:15rpx;
}
.P_bottom {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-left: 20rpx; 
  flex-flow:row;
}
.S_top {
  margin-top: 10rpx;
  background: white;
  padding: 20rpx 40rpx;
}
.S_title {
  font-weight: bold;
  border-bottom: 2rpx solid #ddd;
  padding-bottom: 20rpx;
}
.S_top>.iconfont {
  line-height: 60rpx;
  font-size: 28rpx;
  color: #666;
}
.S_top>.iconfont:before {
  margin-right: 20rpx;
  font-size: 40rpx;
}
.S_top>.icon-zhekou:before {  
  color: #f74d4d;
}

.goods-container image {
  width: 160rpx;
  height: 160rpx;
}
.newUserPag image{
  width: 100rpx;
  height: 150rpx;
  position: fixed;
  bottom: 20%;
  right: 40rpx;
  z-index: 9999999;
}
.openPageBg {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 99999999999;
  background: rgba(0,0,0,.2);
  display: flex;
  justify-content: center;
}
.openPage {
  width: 86%;
  margin-top: 200rpx;
  position: relative;
}
.Picbg {
  width: 100%;
  height: 600rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.openPage .btn {
  position: absolute;
  top: 400rpx;
  right: 0;
  left: 0;
  margin: 0 auto;
  width: 400rpx;
  height: 80rpx;
}
.pageLeft,.pageRight {
  position: absolute;
  z-index: 999999;
}
.pageLeft .price {
  font-size: 60rpx;
}
.smalltext {
  font-size: 32rpx;
}
.pageLeft { 
  width: 140rpx;
  line-height: 64rpx;
  text-align: center;
  top: 70rpx;
  left: 50rpx;
}
.pageRight {
  width: 400rpx;
  line-height: 70rpx;
  top: 70rpx;
  left: 240rpx;
}

.Block {
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background: rgba(0,0,0,.2);
  z-index: 99999999999;
}
.total {
  background: white;
  position: absolute;
  top: 10%;
  left: 5%;
  width: 90%;
}
.title {
  text-align: center;
  padding: 40rpx 0;
}
.title .iconfont {
  font-size: 50rpx;
  float: right;
  margin-right: 40rpx;
}
.type {
  padding: 40rpx;
}
.chooseType {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0 40rpx;
}
.chooseType>view {
  text-align: center;
  width: 30%;
  border: 2rpx solid #333;
  padding: 20rpx 0;
  margin-bottom: 40rpx;
}
.num,.add2 {
  padding: 0 40rpx 40rpx;
}
.ok {
  padding: 40rpx;
  text-align: center;
  border-top: 2rpx solid #ddd;
}
.chooseNum {
  font-size: 48rpx;
  width: 100rpx;
  text-align: center;
  display: inline-block;
}
.add2 .iconfont {
  font-size: 40rpx;
  color: #FFCC33;
}
.content-container {
	display: -webkit-flex;
	height: 100%;
	box-sizing: border-box;
  padding-bottom: 500rpx;
}
.classify-container {
	width: 180rpx;
	background: #efefef;
	height: 100%;
}
.classify-container .classify {
	padding: 40rpx 30rpx;
	text-align: center;
	border-bottom: 1px dashed #aaa;
}
.classify-container .classify.active {
	background: #fff;
}
.classify-container .classify .name {
	display: inline-block;
	font-size: 30rpx;
	color: #646464;
	line-height: 1.2;
	text-align: left;
	pointer-events: none;
}

.goods-container {
	-webkit-flex: 1;
	background: #ffffff;
	height: 100%;
}
.goods-container .title {
	padding: 20rpx 25rpx;
	color: #646464;
	font-size: 30rpx;
	line-height: 30rpx;
	background: #f7f7f7;
}
.goods-container .goods {
	position: relative;
	padding: 15rpx 25rpx;
	font-size: 32rpx;
	line-height: 40rpx;
	border-bottom: 1px solid #f7f7f7;
}
.goods-container .goods .pic {
	float: left;
	width: 120rpx;
	height: 120rpx;
	margin-right: 20rpx;
}
.goods-container .goods .name {
	color: #000;
	font-size: 36rpx;
	line-height: 46rpx;
}
.goods-container .goods .sold {
	color: #989898;
	font-size: 30rpx;
	line-height: 46rpx;
}
.goods-container .goods .price {
	color: #f45044;
	font-size: 32rpx;
	line-height: 46rpx;
}
.goods-container .goods .addCart {
	position: absolute;
	right: 30rpx;
	bottom: 30rpx;
	width: 64rpx;
	height: 64rpx;
	background: #feb70f;
	border-radius: 50%;
}
.goods-container .goods .addCart image {
	pointer-events: none;
	position: absolute;
	left: 20rpx;
	top: 20rpx;
	width: 24rpx;
	height: 24rpx;
}