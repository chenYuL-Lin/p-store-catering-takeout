/**index.wxss**/
page {
  background: #f2f2f2;
  width: 100%;
  height: 100%;
}

.no-login {
  width: 100%;
  height: auto;
  margin: 0 auto;
}

.no-login .c {
  width: 100%;
  height: auto;
  margin-top: 200rpx;
}

.no-login .c image {
  margin: 0 auto;
  display: block;
  text-align: center;
  width: 258rpx;
  height: 258rpx;
}

.no-login .c text {
  margin: 0 auto;
  display: block;
  width: 258rpx;
  height: 59rpx;
  line-height: 29rpx;
  text-align: center;
  font-size: 35rpx;
  color: #999;
}

.no-login button {
  width: 90%;
  margin: 0 auto;
  color: #fff;
  font-size: 30rpx;
  height: 96rpx;
  line-height: 96rpx;
  right: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  position: flex;
  bottom: 0;
  left: 0;
  border-radius: 0;
  padding: 0;
  margin-left: 5%;
  text-align: center;
  /* padding-left: -5rpx; */
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  border-top-right-radius: 50rpx;
  border-bottom-right-radius: 50rpx;
  letter-spacing: 3rpx;
  background-image: linear-gradient(to right, #9a9ba1 0%, #9a9ba1 100%);
}

.no-cart {
  width: 100%;
  height: auto;
  margin: 0 auto;
}

.no-cart .c {
  width: 100%;
  height: auto;
  margin-top: 200rpx;
}

.no-cart .c image {
  margin: 0 auto;
  display: block;
  text-align: center;
  width: 258rpx;
  height: 258rpx;
}

.no-cart .c text {
  margin: 0 auto;
  display: block;
  width: 258rpx;
  height: 29rpx;
  line-height: 29rpx;
  text-align: center;
  font-size: 29rpx;
  color: #999;
}


/*buycar  */
.page2 .main {
  background: #f2f2f2 !important;
  width: 100%;
  height: 100%;
}
.page2 .icon-true {
  color: #999;
}
.page2 .active {
  color: #FFCC33!important;
}
.page2 .pro {
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-around;
  border-top: 2rpx solid #f2f2f2;
  padding: 20rpx 0;
  font-size: 32rpx;
}
.page2 .pro>.icon-true {
  color: #ddd;
}
.page2 .pro .iconfont {
  font-size: 50rpx;
  padding: 0 30rpx;  
  color: #666;
}
.page2 .pro .icon-true {
  color: #ddd;
}
.page2 .pro:nth-child(1) {
  margin-top: 10rpx;
}
.page2 .padding {
  padding-bottom: 230rpx;
}
.page2 .pro image {
  width: 240rpx;
  height: 170rpx; 
}
.page2 .settlement {
  font-size: 32rpx;
  position: fixed;
  width: 100%;
  bottom: 0rpx;
  left: 0;
  background: white;
  border-top: 2rpx #ddd solid;
}
.page2 .tlement {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.page2 .tlement .iconfont {
  font-size: 40rpx;
}
.page2 .message {
  width: 400rpx;  
  margin-left: 20rpx;
}
.page2 .orderNum {
  padding: 0 30rpx;
}
.page2 .message>view {
  line-height: 60rpx;
  display: flex;
  justify-content: space-between;
}
.page2 .message .specification{
  line-height: 50rpx;
  font-size: 26rpx;
  color: #ccc;
}
.orderLeft {
  display: flex;
  align-items: center;
}
.otherMoney {
  padding-top: 10rpx;
  color: #666;
  font-size: 28rpx;
}
.page2 .buycar_icon {
  position: relative;
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  line-height: 90rpx;
  border-radius: 50%;
  background: #FFCC33;
  margin-left: 40rpx;
}
.page2 .buycar_num {
  text-align: center;
  line-height: 28rpx;
  background: #f54949;
  color: white;
  border-radius: 50%;
  width: 28rpx;
  height: 28rpx;
  font-size: 20rpx;
  position: absolute;
  top: 10rpx;
  right: 10rpx;
}
.page2 .totalMoney {
  margin-left: 40rpx;
}
.page2 .toPay {
  padding: 40rpx 80rpx;
  background: #FFCC33;
}
.numBorder {
  display: inline-block;
  border: 2rpx solid #ddd;
  width: 60rpx;
  height: 60rpx;
  text-align: center;
  line-height: 66rpx;
}
.numMiddle {
  display: inline-block;
  width: 70rpx;
  text-align: center;
  height: 60rpx;
  line-height: 66rpx;
  border-top: 2rpx solid #ddd;
  border-bottom: 2rpx solid #ddd;
}